{
  "name": "sairahut-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm exec nx run-many --target=dev",
    "start": "pnpm exec nx run-many --target=start",
    "build": "pnpm exec nx run-many --target=build",
    "deploys": "pnpm exec nx run-many --target=deploys",
    "format": "prettier \"{apps,libs}/*/**.{ts,tsx,js,json}\" --ignore-path .gitignore",
    "format:check": "pnpm format --check",
    "format:write": "pnpm format --write",
    "lint": "pnpm exec nx run-many --target=lint",
    "tsc": "pnpm exec nx run-many --target=tsc",
    "validate": "pnpm format:write && pnpm tsc && pnpm lint",
    "prepare": "husky install",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@nx/eslint": "19.6.0",
    "@nx/next": "19.6.0",
    "autoprefixer": "10.4.13",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.9",
    "nx": "19.6.0",
    "pinst": "^3.0.0",
    "postcss": "8.4.38",
    "prettier": "^3.3.3",
    "tailwindcss": "3.4.3"
  }
}
